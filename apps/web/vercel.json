{
  "redirects": [
    {
      "source": "/home",
      "destination": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "downloads.sunhouse.tech"
        }
      ],
      "destination": "/downloads/:path*"
    }
  ]
}
